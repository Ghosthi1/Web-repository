
function onClick() {
   var button = document.getElementById("ChangeColor");
   button.addEventListener("click", onClick);

   //gets the current color of body
   var bodyElement = document.getElementById("Body");
   //Assigns the body color 
   var currentColor = bodyElement.style.backgroundColor;

   //Checks the current color and changes it appropratly 
   switch ( currentColor ){
      case "":
      case "rgb(213, 213, 213);":
         bodyElement.style.backgroundColor = "rgb(0,0,0)";
      break;     
      case "rgb(0,0,0)":
         bodyElement.style.backgroundColor = "rgb(213, 213, 213)";
      break;
      //Runs this if there is no color applied 
      default:
         bodyElement.style.backgroundColor = "rgb(213, 213, 213)";
      break;
   }
}



//self calling
 (function(){
      document.body.style.height = '200pt';

      var script = document.createElement('script');
      script.type = 'text/javascript';
      script.src = 'https://cdnjs.cloudflare.com/ajax/libs/cookie-banner/1.2.2/cookiebanner.min.js';
      script.id = 'cookiebanner';
      script['data-height']   = '20px';
      script['data-position'] = 'top';
      script['data-message']  = 'We use cookies!';
      document.head.appendChild(script); 

      script.onload = function()
      {
      console.log('loaded script');

      // console.log( initCookieConsent );
      }
      console.log('ready');
   })();
      
